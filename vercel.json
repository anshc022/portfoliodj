{
    "version": 2,
    "builds": [
        {
            "src": "dennisivy/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "dennisivy/wsgi.py"
        }
    ],
    "build": {
        "env": {
            "PG_CONFIG": "/usr/bin/pg_config"
        },
        "commands": [
            "apt-get update && apt-get install -y postgresql-server-dev-all",
            "pip install -r requirements.txt"
        ]
    }
}